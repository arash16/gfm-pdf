{
  "name": "gfm-pdf",
  "version": "1.0.7",
  "description": "A CLI tool to convert Markdown files to PDF with support for math formulas, Mermaid diagrams, and syntax highlighting",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/arash16/gfm-pdf.git"
  },
  "homepage": "https://github.com/arash16/gfm-pdf#readme",
  "bugs": {
    "url": "https://github.com/arash16/gfm-pdf/issues"
  },
  "author": "Arash Shakery <arash.shakery@gmail.com> (arash16.com)",
  "license": "MIT",
  "keywords": [
    "markdown",
    "pdf",
    "cli",
    "converter",
    "mermaid",
    "math",
    "katex",
    "playwright"
  ],
  "packageManager": "yarn@1.22.22",
  "main": "dist/index.js",
  "files": [
    "dist/*.js"
  ],
  "bin": {
    "gfm-pdf": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "postinstall": "playwright install",
    "prepublish": "yarn build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "quality": "yarn type-check && yarn lint && yarn format:check"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "katex": "0.16.22",
    "playwright": "^1.40.1",
    "rehype-katex": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/remark-prism": "^1.3.7",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
